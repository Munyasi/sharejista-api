{
  "name": "ShareTransfer",
  "plural": "sharetransfers",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "forceId": false,
  "mixins": {
    "TimeStamp": true
  },
  "properties": {
    "transferer_id": {
      "type": "number",
      "required": false
    },
    "transferer_type": {
      "type": "string",
      "required": false,
      "description": "Can be company/shareholder"
    },
    "transferee_id": {
      "type": "number",
      "required": true
    },
    "company_id": {
      "type": "number",
      "required": true
    },
    "number_of_shares": {
      "type": "number",
      "required": true
    },
    "share_type_id": {
      "type": "number",
      "required": true
    },
    "approved": {
      "type": "boolean",
      "required": false,
      "default": 0
    },
    "initiated_by": {
      "type": "number",
      "required": false
    },
    "approved_by": {
      "type": "number",
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "transferer": {
      "type": "belongsTo",
      "model": "Shareholder",
      "foreignKey": "transferer_id"
    },
    "transferee": {
      "type": "belongsTo",
      "model": "Shareholder",
      "foreignKey": "transferee_id"
    },
    "sharetype": {
      "type": "belongsTo",
      "model": "ShareType",
      "foreignKey": "share_type_id"
    }
  },
  "acls": [],
  "methods": {}
}
