{
  "name": "Shares",
  "plural": "shares",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "forceId": false,
  "mixins": {
    "TimeStamp": true
  },
  "indexes": {
    "certificate_shareholder_index": {
      "keys": {"certificate_no": 1, "shareholder_id": -1},
      "options": {
        "unique": false
      }
    }
  },
  "properties": {
    "certificate_no": {
      "type": "number",
      "required": true
    },
    "number_of_shares": {
      "type": "number",
      "required": true
    },
    "action": {
      "type": "string",
      "required": true,
      "desc": "Expects TRANSFER/ALLOTMENT/REPLACEMENT/CF(carried forward from manual system)"
    },
    "status": {
      "type": "string",
      "required": true,
      "desc": "Expects VALID/INVALID"
    },
    "dated": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "Shareholder": {
      "type": "belongsTo",
      "model": "Shareholder",
      "foreignKey": "shareholder_id"
    },
    "ShareType": {
      "type": "belongsTo",
      "model": "ShareType",
      "foreignKey": "sharetype_id"
    },
    "ShareTransfer": {
      "type": "belongsTo",
      "model": "ShareTransfer",
      "foreignKey": "sharetransfer_id"
    },
    "Company": {
      "type": "belongsTo",
      "model": "Company",
      "foreignKey": "company_id"
    }
  },
  "acls": [],
  "methods": {}
}
